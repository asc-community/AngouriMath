<?xml version="1.0" encoding="utf-8"?>
<Project>
  
  <!--
  
  Here we store the info about packages produced in AngouriMath.
  This contains general info which is the same for all packages.
  
  This should not store anything specific to any package, including
  the output type, target frameworks.
  
  -->
  
  <PropertyGroup>
    <Version>1.4.0-preview.3</Version>

    <Authors>WhiteBlackGoose and contributors</Authors>
    <Company>Angouri</Company>
    <Copyright>Â© Angouri 2019-2022</Copyright>

    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>

    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>See it on the website https://am.angouri.org/whatsnew</PackageReleaseNotes>
    <PackageProjectUrl>https://am.angouri.org</PackageProjectUrl>
    <RepositoryUrl>https://github.com/asc-community/AngouriMath</RepositoryUrl>
    <PackageTags>math, symbolics, latex, evaluate, evaluation, dotnet, parse, simplify, derivation, equation, calculus, symbolic algebra</PackageTags>
    
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <PackageIcon>icon.png</PackageIcon>

    <PackageReadmeFile>PackageReadme.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="icon.png" Pack="True" PackagePath="" />

    <None Include="PackageReadme.md" Pack="True" PackagePath="" />
  </ItemGroup>
</Project>